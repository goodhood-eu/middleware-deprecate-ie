{
  "name": "middleware-deprecate-ie",
  "description": "ExpressJS middleware that shows a landing screen to all Internet Explorer users preventing them from accessing the website",
  "author": "Max Degterev <max@nebenan.de>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "repository": "goodhood-eu/middleware-deprecate-ie",
  "bugs": "https://github.com/goodhood-eu/middleware-deprecate-ie/issues",
  "version": "2.0.0-beta.0",
  "keywords": [
    "express",
    "expressjs",
    "middleware",
    "internet explorer",
    "deprecate"
  ],
  "main": "index.js",
  "scripts": {
    "start": "npm run compile:watch & nodemon demo.js",
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "check": "npm run lint && echo \"\\033[0;32mReady to publish\\033[0m\"",
    "clean": "rm -rf ./dist/ || exit 1",
    "compile": "NODE_ENV=production babel ./client/ --out-dir ./dist/ --copy-files",
    "compile:watch": "NODE_ENV=production babel --watch ./client/ --out-dir ./dist/ --copy-files",
    "minify": "find ./dist/ -name '*.js' -exec terser {} -o {} --config-file terser.json \\;",
    "build": "npm run compile && npm run minify",
    "prepublishOnly": "npm run check && npm run clean && npm run build",
    "lint": "eslint --ext .es,.js ./"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "eslint": "^6.8.0",
    "eslint-config-nebenan-base": "^1.3.1",
    "express": "^4.17.1",
    "nodemon": "^2.0.3",
    "serve-static": "^1.14.1",
    "terser": "^4.6.11"
  },
  "dependencies": {
    "lodash.get": "4.4.2"
  }
}
